<!--Render register page form-->
<%- include("chunks/form-wrapper") %>
<% scripts = `
<script type="module">
   import showPopup from "./js/popup.js";
   import {showPassword, hidePassword} from "./js/showPassword.js";
   showPassword();
   hidePassword();

   const urlParams = new URLSearchParams(window.location.search);
   if (urlParams.has("error")) {
        showPopup("Wrong email or password. Please check them and try again", "error", "center");
   }
</script>
`; %>