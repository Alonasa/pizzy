<!--Render register page form-->
<%- include("chunks/form-wrapper") %>
<% scripts = `
<script type="module">
   import showPopup from "./js/popups.js";

    //Show the modal if there's an error in the URL
    const urlParams = new URLSearchParams(window.location.search);
    if (urlParams.has("email")) {
        const passwordElement = document.querySelector("#email");

        passwordElement.value = urlParams.get("email");
    }
</script>
`; %>